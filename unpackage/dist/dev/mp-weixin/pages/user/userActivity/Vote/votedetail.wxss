.home {
  width: 750rpx;
}
.home .voteBox {
  width: 640rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  border-radius: 30rpx;
  background: #fff;
  padding: 0 30rpx 20rpx 30rpx;
}
.home .voteBox .vote_title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
  height: 60rpx;
  border-bottom: 2rpx solid #C8C7CC;
}
.home .voteBox .vote_title image {
  width: 40rpx;
  height: 50rpx;
  margin-left: 10rpx;
}
.home .voteBox .vote_title .vote_title_text {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 90%;
  height: 60rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.home .voteBox .vote_title .vote_title_text text {
  font-size: 28rpx;
  font-weight: 600;
  line-height: 60rpx;
  margin-left: 20rpx;
}
.home .voteBox .vote .votetop {
  margin-top: 15rpx;
  font-size: 36rpx;
  font-weight: 600;
}
.home .voteBox .vote .votemid {
  margin-top: 15rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 28rpx;
}
.home .voteBox .vote .votemid text {
  display: block;
}
.home .voteBox .vote .votemid text:nth-child(2) {
  display: block;
  color: #666666;
}
.home .voteBox .vote .votebot {
  margin-top: 15rpx;
  font-size: 28rpx;
}
.home .votedetail {
  width: 690rpx;
  margin: 0 auto;
  border-radius: 30rpx;
  background-color: #fff;
}
.home .votedetail .detail_item {
  width: 100%;
  height: 40rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-top: 50rpx;
}
.home .votedetail .detail_item text {
  font-size: 28rpx;
}
.home .votedetail .detail_item text:nth-child(1) {
  width: 150rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
}
.home .votedetail .detail_item .detail_tit {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.home .votedetail .detail_item .detail {
  width: 440rpx;
}
.home .votedetail .detail_item .detail .detail_clu {
  margin-right: 20rpx;
  height: 40rpx;
  background-color: #6D9EF4;
}
.home .votedetail .detail_item .detail_num {
  width: 100rpx;
  text-align: center;
}

