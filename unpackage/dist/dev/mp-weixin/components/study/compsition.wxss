.container {
  width: 750rpx;
}
.container .container-item {
  padding: 48rpx 0rpx 42rpx 51rpx;
  position: relative;
}
.container .container-item .date {
  display: inline-block;
  margin-bottom: 26rpx;
  font-size: 30rpx;
  line-height: 24rpx;
  color: black;
}
.container .container-item .test-time {
  display: inline-block;
  font-size: 28rpx;
  color: #383838;
}
.container .container-item .score-look {
  display: inline-block;
  margin-left: 140rpx;
  font-size: 24rpx;
  color: #2f2fff;
}
.container .container-item .test-start {
  position: absolute;
  top: 38rpx;
  right: 39rpx;
  width: 158rpx;
  height: 58rpx;
  text-align: center;
  background: #dd514c;
  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.16);
  opacity: 1;
  border-radius: 12px;
}
.container .container-item .test-start text {
  font-size: 28rpx;
  height: 37rpx;
  line-height: 58rpx;
}

