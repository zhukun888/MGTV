<view class="content"><block wx:for="{{vote}}" wx:for-item="item" wx:for-index="id" wx:key="id"><view class="content_item"><view><text>{{item.title}}</text><text>{{"投票时间:"+item.begin_time+"-"+item.end_time}}</text><text>{{"参加人数:"+item.join_num+"人"}}</text></view><block wx:if="{{item.time_state==2}}"><view data-event-opts="{{[['tap',[['navItemClick',['$0'],[[['vote','',id,'id']]]]]]]}}" bindtap="__e">进行中</view></block><block wx:if="{{item.time_state==3}}"><view data-event-opts="{{[['tap',[['navItemClick',['$0'],[[['vote','',id,'id']]]]]]]}}" bindtap="__e">已结束</view></block><block wx:if="{{item.time_state==1}}"><view data-event-opts="{{[['tap',[['navItemClick',['$0'],[[['vote','',id,'id']]]]]]]}}" bindtap="__e">未开始</view></block><view data-event-opts="{{[['tap',[['navItemClick',['$0'],[[['vote','',id,'id']]]]]]]}}" bindtap="__e">></view></view></block></view>